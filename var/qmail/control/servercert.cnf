RANDFILE = ./servercert.rand

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = mx.pupkin.org

[ req ]
default_bits = 4096
encrypt_key = yes
distinguished_name = req_dn
x509_extensions = cert_type
prompt = no
default_md = sha1

[ req_dn ]
C=RU
ST=Muhosransk city
L=Muhosransk
O=Pupkin
OU=Automatically-generated SSL key
CN=mx.pupkin.org
emailAddress=postmaster@pupkin.org


[ cert_type ]
nsCertType = server
